      }
    },
    "variables" : {
        "f" : {
            "type" : "INT",
            "size" : 1,
            "min"  : 70,
            "max"  : 200
        },
      	"x" : {
      	    "type" : "FLOAT",
      	    "size" : 1,
      	    "min" : 0.2,
      	    "max" : 3
      	},
        "m" : {
            "type" : "INT",
            "size" : 1,
            "min" : 6,
            "max" : 50
        }
    }
}
