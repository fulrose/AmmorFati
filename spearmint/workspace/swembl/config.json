{
    "language"        : "PYTHON",
    "experiment-name" : "swembl_test",
    "resources" : {
        "my-machine" : {
            "scheduler"         : "local",
            "max-concurrent"    : 4,
            "max-finished-jobs" : 12
        }
    },
    "tasks": {
      "branin" : {
          "type"       : "OBJECTIVE",
