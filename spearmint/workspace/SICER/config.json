{
    "language"        : "PYTHON",
    "experiment-name" : "sicer_test",
    "polling-time"    : 1,
    "resources" : {
        "my-machine" : {
            "scheduler"         : "local",
            "max-concurrent"    : 4,
            "max-finished-jobs" : 100
        }
    },

    "tasks": {
      "branin" : {
          "type"       : "OBJECTIVE",
          "likelihood" : "NOISELESS",
          "main-file"  : "sicer.py",
          "resources"  : ["my-machine"]
      }
    },

    "variables" : {
        "gs" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 0.1,
            "max"  : 1
        },
      	"w" : {
      	    "type" : "INT",
      	    "size" : 1,
      	    "min" : 100,
      	    "max" : 400
       	},
        "fs" : {
            "type" : "INT",
            "size" : 1,
            "min" : 100,
            "max" : 400
      }
    }
  }
